		<div data-role="page" id="addTripPage" >
			<header data-role="header" data-position="fixed" data-tap-toggle="false">     
				<h1><a href="#">Tripper</a></h1>
				<ul class="nav nav-pills">
					<li id="home"><a href="#homePage">חיפוש טיול</a></li>
					<li id="showTrips"><a href="#accountPage">החשבון שלי</a></li>
					<li id="moveToFavorite"><a href="#viewFavorite">המועדפים שלי</a></li>
					<li id="mySchedule"><a href="#myPageSchedule">המסלול שלי</a></li>
					<li id="addTrip"><a href="#addTripPage">הוספת טיול</a></li>
				</ul>
			</header>

			<section id="addTripSec">
				<form action="/add" method="post" onsubmit="return validateMyForm(this);">
					<label>שם או אזור הטיול <br>
						<input type="text" name="nameTrip">
					</label><br>
					<label>תאור הטיול<br>
						<textarea type="text" name="des"></textarea>
					</label><br>

					<div data-role="fieldcontain">
						<fieldset data-role="controlgroup" data-type="horizontal" data-role="fieldcontain">
							<legend>אזור הטיול:</legend>
							<input type="radio" name="area" id="radio-choice-1" value="tel-aviv" checked="checked" />
							<label for="radio-choice-1">אזור המרכז</label>

							<input type="radio" name="area" id="radio-choice-2" value="south"  />
							<label for="radio-choice-2">אזור הדרום</label>

							<input type="radio" name="area" id="radio-choice-3" value="north"  />
							<label for="radio-choice-3">אזור הצפון</label>

						</fieldset>
					</div>	

					<label>מיקום הטיול: <br>
						<input type="text" name="locationTrip">
					</label><br>
					<label>תכונות הטיול: <br>
						<input type="text" name="char1">
						<input type="text" name="char2">
					</label>

					<label for="ingredients"> <span class="testRight" id="ingredientsT">אתרי תיירות</span>
						<span class="formField">
							<div class="ingredients_wrap">

								<div style="text-align: center;margin: auto;position: relative;align-content: center;">
									<input type="text" name="ingredients[]" placeholder="אתר" class="firstIngredient ingredients_i" id="firstIngredient">
									<input type="text" name="amount[]" placeholder="מיקום" class="firstAmount amounts_i" id="firstAmount">
								</div>
							</div> 
						</span> 
					</label>
					<br>
					<!--<input name="image_file" id="imageInput" type="file" />
					<img src="images/ajax-loader.gif" id="loading-img" style="display:none;" alt="Please Wait"/>
					<div id="output"></div>-->

					<!--	<input type='file' id="imgInp" />
   					<img id="blah" src="#" alt="your image" />-->

   					<input type="file" accept="image/*"  onchange="showMyImage(this)" />
					 <br/>
					<img id="thumbnil" style="width:20%; margin-top:10px;"  src="" alt="image"/>

					<button type="submit" data-theme="b" name="submit" value="submit-value" class="ui-btn-hidden" aria-disabled="false">שלח טיול</button>
					<input type="hidden" name="commodities" id="commodities" value=""/>
				</form>
			</section>
		</div>

		<div data-role="page" id="accountPage" >
			<header data-role="header" data-position="fixed" data-tap-toggle="false">     
