		<div data-role="page" id="resultTripPqge" >
			<header data-role="header" data-position="fixed" data-tap-toggle="false">     
				<h1><a href="#">Tripper</a></h1>
				<ul class="nav nav-pills">
					<li id="home"><a href="#homePage">חיפוש טיול</a></li>
					<li id="showTrips"><a href="#accountPage">החשבון שלי</a></li>
					<li id="moveToFavorite"><a href="#viewFavorite">המועדפים שלי</a></li>
					<li id="mySchedule"><a href="#myPageSchedule">המסלול שלי</a></li>
					<li id="addTrip"><a href="#addTripPage">הוספת טיול</a></li>
				</ul>
			</header>
			<input type="button" id="filter" value="filter">
			<div id="filteroptions" class="hidden">
				<label for="slider-1">בחר תקציב:</label>
				<input type="range" name="slider-1" id="slider-1" value="60" min="0" max="100" />	
				<div data-role="fieldcontain">
    				<fieldset data-role="controlgroup" data-type="horizontal">
	   				<legend>עם מי הולכים לטייל?</legend>
	  				<input type="checkbox" name="trip_with_animals" id="trip_with_animals" class="custom" />
	   				<label for="trip_with_animals">בע"ח</label>
	   				<input type="checkbox" name="trip_with_road" id="trip_with_road" class="custom" />
	   				<label for="trip_with_road">מסלול סלול לאנשים עם מוגבליות או עגלות לתינוקות</label>
	   				<input type="checkbox" name="trip_for_children" id="trip_for_children" class="custom" />
	   				<label for="trip_for_children">מותאם לילדים</label>
   					</fieldset>
				</div>
				<div data-role="fieldcontain">
    				<fieldset data-role="controlgroup" data-type="horizontal">
	   				<legend>סוג טיול</legend>
	  				<input type="checkbox" name="trip_with_watter" id="trip_with_watter" class="custom" />
	   				<label for="trip_with_watter">טיול עם מקורות מים</label>
	   				<input type="checkbox" name="trip_for_night" id="trip_for_night" class="custom" />
	   				<label for="trip_for_night">טיול עם לילה</label>
	   				<input type="checkbox" name="trip_for_day" id="trip_for_day" class="custom" />
	   				<label for="trip_for_day">טיול יומי</label>
	   				<input type="checkbox" name="trip_with_bicycle" id="trip_with_bicycle" class="custom" />
	   				<label for="trip_with_bicycle">טיול עם אופניים</label>
	   				<input type="checkbox" name="trip_with_jeep" id="trip_with_jeep" class="custom" />
	   				<label for="trip_with_jeep">טיול ג'יפים</label>
   					</fieldset>
				</div>
				<div data-role="fieldcontain">
    				<fieldset data-role="controlgroup" data-type="horizontal">
	   				<legend>בחר רמת קושי</legend>
	  				<input type="checkbox" name="light_trip" id="light_trip" class="custom" />
	   				<label for="light_trip">קל</label>
	   				<input type="checkbox" name="medium_trip" id="medium_trip" class="custom" />
	   				<label for="medium_trip">בינוני</label>
	   				<input type="checkbox" name="hard_trip" id="hard_trip" class="custom" />
	   				<label for="hard_trip">קשה</label>
   					</fieldset>
				</div>
			</div>
				<div data-role="content" class="ui-content" id="content">
					<select id="selectArea" onchange="changedArea()">
						<option value=""></option>
						<option value="tel-aviv">איזור מרכז וגוש דן</option>
						<option value="south">איזור דרום</option>
						<option value="north">איזור צפון</option>
					</select>
					<section id="resultTrip">
						<ul class="displayTrip">
						</ul>

					</section>
				</div>
			</div>


		<div data-role="page" id="theTrip" >
